script/source = "class_name BigGoblinState
extends State

# State machine parent node sets this, so that state can call its state machine's transition_to() function
#var state_machine = null
var biggoblin: BigGoblin

func _ready() -> void:
	await owner.ready
	biggoblin = owner as BigGoblin
	assert(biggoblin != null)

# Receives events from the `_unhandled_input()` callback.
func handle_input(_event: InputEvent) -> void:
	pass


# Corresponds to the `_process()` callback.
func update(_delta: float) -> void:
	biggoblin.animated_sprite.play(\"biggoblin_walk\")


# Corresponds to the `_physics_process()` callback.
func physics_update(_delta: float) -> void:
	calculate_movement()

	if biggoblin.distance_to_player >= biggoblin.approach_range:
		state_machine.transition_to(\"Idle\")

	if biggoblin.distance_to_player <= biggoblin.attack_range:
		state_machine.transition_to(\"Attack\")

func calculate_movement():
	calculate_knockback()
	calculate_velocity()
	face_movement_direction()
	biggoblin.move_and_slide()


func point_toward(target):
	biggoblin.direction = biggoblin.global_position.direction_to(target.global_position)


func calculate_knockback():
	biggoblin.knockback = biggoblin.knockback.move_toward(Vector2.ZERO, biggoblin.knockback_recovery)
	biggoblin.velocity += biggoblin.knockback


func calculate_velocity():
	point_toward(biggoblin.player)
	biggoblin.velocity = biggoblin.direction * biggoblin.movement_speed


func face_movement_direction():
	if biggoblin.velocity.x != 0:
		if biggoblin.direction.x < 0:
			biggoblin.sprite_container.scale.x = -1
		else:
			biggoblin.sprite_container.scale.x = 1


# Called by the state machine upon changing the active state. The `msg` parameter is a dictionary with arbitrary data the state can use to initialize itself.
func enter(_msg := {}) -> void:
	pass


# Called by the state machine before changing the active state. Use this function to clean up the state.
func exit() -> void:
	pass
"

